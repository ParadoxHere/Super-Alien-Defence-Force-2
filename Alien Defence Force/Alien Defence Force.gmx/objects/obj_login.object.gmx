//Async HTTP event
var resultid= gj_result_id();

//If it was user information.
    if(!gj_user_isloggedin()) {
        var status=gj_result_status();
        if(status) {
            canproceed=true;
            
            if(gj_user == "pewdiepie") {
                global.petUnlocked[pet.pug] = true;
                global.pet = pet.pug;
                save_game();
            }
            
            if(gj_user == "Markimurder") {
                global.petUnlocked[pet.mustache] = true;
                global.pet = pet.mustache;
                save_game();
            }
            
            if(gj_user == "therealjacksepticeye") {
                global.petUnlocked[pet.eye] = true;
                global.pet = pet.eye;
                save_game();
            }
            
            if(instance_exists(obj_remember_me)) {
                if(obj_remember_me.image_index == 1) {
                    gj_user_save_details();
                }
            }
            room_goto(rm_main_menu);
        } else if(status==0) {
            global.gj_message = "Username or password is incorrect.";
        } else {
            global.gj_message = "GameMaker error.";
        }
    }
</string>
